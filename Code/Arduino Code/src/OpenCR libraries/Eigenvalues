#ifndef EIGEN_EIGENVALUES_MODULE_H
#define EIGEN_EIGENVALUES_MODULE_H

#include <Core>

#include "DisableStupidWarnings.h"

#include <Cholesky>
#include <Jacobi>
#include <Householder>
#include <LU>
#include <Geometry>

/** \defgroup Eigenvalues_Module Eigenvalues module
  *
  *
  *
  * This module mainly provides various eigenvalue solvers.
  * This module also provides some MatrixBase methods, including:
  *  - MatrixBase::eigenvalues(),
  *  - MatrixBase::operatorNorm()
  *
  * \code
  * #include <Eigen/Eigenvalues>
  * \endcode
  */

#include "RealSvd2x2.h"
#include "Tridiagonalization.h"
#include "RealSchur.h"
#include "EigenSolver.h"
#include "SelfAdjointEigenSolver.h"
#include "GeneralizedSelfAdjointEigenSolver.h"
#include "HessenbergDecomposition.h"
#include "ComplexSchur.h"
#include "ComplexEigenSolver.h"
#include "RealQZ.h"
#include "GeneralizedEigenSolver.h"
#include "MatrixBaseEigenvalues.h"
#ifdef EIGEN_USE_LAPACKE
#include "src/misc/lapacke.h"
#include "src/Eigenvalues/RealSchur_LAPACKE.h"
#include "src/Eigenvalues/ComplexSchur_LAPACKE.h"
#include "src/Eigenvalues/SelfAdjointEigenSolver_LAPACKE.h"
#endif

#include "ReenableStupidWarnings.h"

#endif // EIGEN_EIGENVALUES_MODULE_H
